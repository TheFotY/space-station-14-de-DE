cmd-announce-desc = Sende eine Spielankündigung mit benutzerdefinierter Farbe und Sound.
cmd-announce-help = {$command} <Nachricht> [Absender] [Farbe] [Sound] - Sende Ankündigung. Absender ist standardmäßig CentCom, Farbe ist Gold, Sound ist announce.ogg. Die Farbe sollte im #RRGGBB-Format sein.

# Der Standard-Absender für die Ankündigung
cmd-announce-sender = Zentralkommando

# Vervollständigungshinweise
cmd-announce-arg-message = <Nachricht>
cmd-announce-arg-sender = [Absender]
cmd-announce-arg-color = [Farbe]
cmd-announce-arg-sound = [Sound]
